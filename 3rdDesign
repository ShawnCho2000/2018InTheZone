	}

	ret = programmingSkill_Pick_2nd_MG();
	if (!ret){
		return;
	}

	ret = programmingSkill_Pick_3rd_MG(); // Pick up 3rd MG
	if (!ret){
		return;
	}

	stopAll();
}

