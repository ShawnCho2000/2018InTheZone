	wait1Msec(GLOBAL_WAITER);
	moveMobileLiftUpAndStop();
	wait1Msec(1000);



	bool ret = Prepare_2ND_MG();
	if (!ret){
		return;
	}

	ret = programmingSkill_Pick_2nd_MG();
	if (!ret){
		return;
	}




}

