void autonRed2() {
	closeClaw(10);
	wait1Msec(300);
	MoveHelper(30);
	moveLiftUpForStationaryGoalAuton(STATIONARY_LIFT);
	MoveIEM(300, -30);
	wait1Msec(GLOBAL_WAITER);
	//TurnGyro(750, 70);
	//wait1Msec(GLOBAL_WAITER + 200);
	//MoveAccel(700, 50);
	stopAll();
}

