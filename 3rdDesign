/*---------------------------------------------------------------------------*/
/*                          Pre-Autonomous Functions                         */
/*                                                                           */
/*  You may want to perform some actions before the competition starts.      */
/*  Do them in the following function.  You must return from this function   */
/*  or the autonomous and usercontrol tasks will not be started.  This       */
/*  function is only called once after the cortex has been powered on and    */
/*  not every time that the robot is disabled.                               */
/*---------------------------------------------------------------------------*/

void pre_auton()
{
	// Set bStopTasksBetweenModes to false if you want to keep user created tasks
	// running between Autonomous and Driver controlled modes. You will need to
	// manage all user created tasks if set to false.
	bStopTasksBetweenModes = true;

	SensorValue[lineBack] = sensorNone;
	SensorValue[lineRight] = sensorNone;
	SensorValue[lineLeft] = sensorNone;
	SensorValue[GyroPosition] = sensorNone;
	wait1Msec(1000);


	SensorValue[GyroPosition] = sensorGyro;
	SensorValue[lineBack] = sensorLineFollower;
	SensorValue[lineRight] = sensorLineFollower;
	SensorValue[lineLeft] = sensorLineFollower;
	wait1Msec(1000);

	SensorFullCount[GyroPosition] = 3600;
	SensorValue[GyroPosition] = 0;
	nMotorEncoder[wheelRight] = 0;

	COLOR_BASE_BACK = SensorValue[lineBack];
	COLOR_BASE_RIGHT = SensorValue[lineRight];
	COLOR_BASE_LEFT = SensorValue[lineLeft];


	// startAuton(FULLPOWER);

	// Set bDisplayCompetitionStatusOnLcd to false if you don't want the LCD
	// used by the competition include file, for example, you might want
	// to display your team name on the LCD in this function.
	// bDisplayCompetitionStatusOnLcd = false;

	// All activities that occur before the competition starts
	// Example: clearing encoders, setting servo positions, ...

}

bool startAuton(int power) {

	clearTimer(T2);
	int tcur = getLiftPosition();
	while (getLiftDown() == 0 && tcur <= MIN_LIFT){
		// wait1Msec(50);

		moveLift(-power);
		if (vexRT[Btn8L] == 1 || time1[T2] > 500)
		{
			stopLift();
			return false;
		}
	}

	stopLift();

	return true;
}


/*---------------------------------------------------------------------------*/
/*                                                                           */
/*                              Autonomous Task                              */
/*                                                                           */
/*  This task is used to control your robot during the autonomous phase of   */
/*  a VEX Competition.                                                       */
/*                                                                           */
/*  You must modify the code to add your own robot specific commands here.   */
/*---------------------------------------------------------------------------*/

task autonomous()
{

	long startTime = nPgmTime;

	startAuton(FULLPOWER);

	if (autonomousMode == 1)
	{
		autonRed();
	}
	else if (autonomousMode == 2)
	{
		autonRed2();
	}
	else if (autonomousMode == 3)
	{
		programmingSkill();
	}


	writeDebugStreamLine("autonomous) Time: %d", nPgmTime - startTime);

	// ..........................................................................
	// Insert user code here.
	// ..........................................................................

	// Remove this function call once you have "real" code.

}

/*---------------------------------------------------------------------------*/
/*                                                                           */
/*                              User Control Task                            */
/*                                                                           */
/*  This task is used to control your robot during the user control phase of */
/*  a VEX Competition.                                                       */
/*                                                                           */
/*  You must modify the code to add your own robot specific commands here.   */
/*---------------------------------------------------------------------------*/

task move()
{
	while (true)
	{
		wait1Msec(50);

		int ch1 = vexRT[Ch1];
		int ch3 = vexRT[Ch3];

		if (autonomousMode == 5)
		{
			ch1 = ch1 * 3 / 5;
			ch3 = ch3 * 2 / 3; //fastest pair was 3/5 for turning and 4/5 for going back and forth
		}

		motor[wheelLeft] = -ch3 - ch1;
		motor[wheelRight] = ch3 - ch1;


		if (abs(motor[wheelRight]) < 17 || abs(motor[wheelLeft]) < 17)
		{
			motor[wheelLeft] = 0;
			motor[wheelRight] = 0;
		}
	}
}


task lift()
{
	while (true)
	{
		wait1Msec(50);

		if (vexRT[Btn8R] == 1) {
			if (vexRT[Btn7U] == 1) { //Move Lift up with btn 7U
				LIFT_STATUS = "manual";
				moveLiftUp(FULLPOWER); //80
			}

			else if (vexRT[Btn7D] == 1){ //Move Lift down with btn 7D
				LIFT_STATUS = "manual";
				// moveLiftDownAndHold_Touch(powerliftDown); //-40
				moveLiftDown(powerliftDown); //-40

			}
			else if (vexRT[Btn7R] == 1){
				moveMobileLiftDownAndStop();
				// moveMobileLiftDownToPickUp();
			}
			else if (vexRT[Btn7L] == 1){
				moveMobileLiftUpAndStop();
			}
			else if (vexRT[Btn6U] == 1){
				if (CONE_COUNT < 1){
					moveLiftUpForStationaryGoal(STATIONARY_LIFT);
				}
				else
				{

					int clawLiftPosition = getClawLiftPosition();

					if (clawLiftPosition >= MID_CLAWLIFT){
						scoreStationaryGoal_Part1();
					}
					else if (clawLiftPosition < MID_CLAWLIFT)
					{
						scoreStationaryGoal_Part2();
					}

				}
			}
			else if (vexRT[Btn6D] == 1){
				if (CONE_COUNT < 1){
					moveLiftUpForStationaryGoal(STATIONARY_LIFT);
				}
				else
				{
					bool ret = true;
					while(CONE_COUNT > 1) {
						ret = scoreStationaryGoal_Part1();
						if (!ret){
							break;
						}
						ret = scoreStationaryGoal_Part2();
						if (!ret){
							break;
						}
					}

					stopAll();
				}
			}


			// if (vexRT[Btn5U] == 0 && vexRT[Btn5D] == 0){
			// 	stopClawLift();
			// }

			if (LIFT_STATUS == "manual" && vexRT[Btn7U] == 0 && vexRT[Btn7D] == 0){
				stopLift();
			}

			if (vexRT[Btn7R] == 0 && vexRT[Btn7L] == 0){
				stopMobileLift();
			}
		}
		else if (vexRT[Btn8R] == 0) {
			if (vexRT[Btn7U] == 1) {


				CONE_COUNT++;
				int clawLiftPosition = getClawLiftPosition();

				// int liftPosition = getLiftPosition();

				// if (liftPosition < MIN_LIFT && clawLiftPosition < MIN_CLAWLIFT){
				// 	LiftDownPickUpClawLiftUpLiftDown_PART1_2();
				// }
				// else
				if (clawLiftPosition >= MID_CLAWLIFT){
					LiftDownPickUpClawLiftUpLiftDown_PART1_1(MIN_LIFT, false);
				}
				else if (clawLiftPosition < MID_CLAWLIFT)
				{
					LiftDownPickUpClawLiftUpLiftDown_PART2_1(MIN_LIFT, false);
				}
			}
			else if (vexRT[Btn7D] == 1) {

				int clawLiftPosition = getClawLiftPosition();

				// int liftPosition = getLiftPosition();

				// if (liftPosition < MIN_LIFT && clawLiftPosition < MIN_CLAWLIFT){
				// 	LiftDownPickUpClawLiftUpLiftDown_PART1_2();
				// }
				// else
				if (clawLiftPosition >= MID_CLAWLIFT){
					LiftDownPickUpClawLiftUpLiftDown_PART1_1(MIN_LIFT, false);
				}
				else if (clawLiftPosition < MID_CLAWLIFT)
				{
					LiftDownPickUpClawLiftUpLiftDown_PART2_1(MIN_LIFT, false);
				}
			}
			else if (vexRT[Btn7L] == 1){
				if (autonomousMode == 5){
					moveMobileLiftUpAndStopDrivingSkill();
				}
				else{
					moveMobileLiftUpAndStop();
				}

			}
			else if (vexRT[Btn7R] == 1){
				moveMobileLiftDownToPickUp();
			}
			else if (vexRT[Btn8U] == 1){
				CONE_COUNT++;
				LiftDownPickUpClawLiftUpLiftDown_PART1_1(FEED_LIFT, true);
				LiftDownPickUpClawLiftUpLiftDown_PART2_1(FEED_LIFT, true);
			}
			else if (vexRT[Btn8D] == 1){
				// autonRed();
				LiftDownPickUpClawLiftUpLiftDown_PART1_1(FEED_LIFT, true);
				LiftDownPickUpClawLiftUpLiftDown_PART2_1(FEED_LIFT, true);
			}

			if (vexRT[Btn7R] == 0 && vexRT[Btn7L] == 0){
				stopMobileLift();
			}
		}
	}
}

void LiftDownPickUpClawLiftUpLiftDown_PART1_1(int liftValue, bool manualFeed)
{

	writeDebugStreamLine("PART1");
	// if (clawLiftPosition < MID_CLAWLIFT)
	// {
	// 	return;
	// }

	LIFT_STATUS = "LiftDownPickUpClawLiftUpLiftDown_PART1_1";
	// READY PICKUP
	openClaw(FULLPOWER);
	wait1Msec(300);

	int offset = getLiftTargetPosition(manualFeed);
	int target = liftValue + offset;
	// LIFT_TARGET = target;

	int clawLiftPosition = getClawLiftPosition();
	// writeDebugStreamLine("clawLiftDown: %d", clawLiftDown);


	writeDebugStreamLine("200");
	if (clawLiftPosition >= MID_CLAWLIFT)
	{
		// moveLiftUpAndStop(target, FULLPOWER);
		moveLiftUpAndHold(target, FULLPOWER);

	}

	writeDebugStreamLine("201");

	stopClaw();


	// startTask(moveClawLiftDownAndStopTask);

	// waitUntil(getClawLiftPosition() >= MAX_CLAWLIFT);
	// writeDebugStreamLine("203");


	// moveClawLiftDownAndStop(MIN_CLAWLIFT, powerClawLiftDown);

	moveClawLiftDownAndContinue(MID_CLAWLIFT, powerClawLiftDown);
	startTask(moveClawLiftDownAndStopTask);

	if (!manualFeed)
	{
		writeDebugStreamLine("203");

		bool ret = moveLiftDownAndStop(MIN_LIFT, powerliftDown + 30);
		// bool ret = moveLiftDownAndStop_Touch(powerliftDown + 30);
		if (ret) {
			int tcur = getLiftPosition();
			moveLiftUpAndStop(tcur + 20, 30);
		}
	}

	LIFT_STATUS = "";
}

// void LiftDownPickUpClawLiftUpLiftDown_PART1_2()
// {
// 	// int liftPos = getLiftPosition();
// 	// int clawLiftPos = getClawLiftPosition();

// 	// if (liftPos < MIN_LIFT && clawLiftPos < MIN_CLAWLIFT){
// 	closeClaw(FULLPOWER);
// 	moveLiftDownAndHold_Touch(FULLPOWER);
// 	// }
// }

void LiftDownPickUpClawLiftUpLiftDown_PART2_1(int liftValue, bool manualFeed)
{
	writeDebugStreamLine("PART2");
	LIFT_STATUS = "LiftDownPickUpClawLiftUpLiftDown_PART2_1";

	int offset = getLiftTargetPosition(manualFeed);
	int target = liftValue + offset;
	LIFT_TARGET = target;
	bool ret = true;

	// PICKUP
	closeClaw(FULLPOWER);

	if (manualFeed){
		ret = moveLiftDownAndStop(FEED_LIFT, powerliftDown + 30);
		if (!ret){
			return;
		}

		// PICK UP CONE AND HOLD
		// wait1Msec(450 - 250);
	}
	else {
		// moveLift(-((powerliftDown * 2) / 3));
		// moveLift(-FULLPOWER);
		// wait1Msec(300);
		ret = moveLiftDownAndHold_Touch(FULLPOWER);
		if (!ret){
			return;
		}

		while(true){
			if (vexRT[Btn7D] == 1){
				break;
			}
			// else if (vexRT[Btn7U] == 1){
			// 	stopClaw();
			// 	int liftPos = getLiftPosition();
			// 	moveLiftUpAndStop(liftPos + 20, 30);
			// 	break;
			// }
			else if (vexRT[Btn8L] == 1){
				stopAll();
				return;
			}


		}
		// waitUntil(vexRT[Btn7D] == 1);
	}

	// // PICK UP CONE AND HOLD
	// wait1Msec(450 - 250);

	// CONE HOLD
	closeClaw(10);


	// if (CONE_COUNT < 10){
	// 	moveClawLiftUp(MAX_CLAWLIFT);
	// }

	// int cPower = FULLPOWER;
	// if (CONE_COUNT > 7){
	// 	cPower = FULLPOWER / 2;
	// }

	// moveClawLiftUp(cPower);

	if (CONE_COUNT < 10){
		startTask(moveClawLiftUpAndStopTask);
	}

	if (manualFeed){
		if (CONE_COUNT >= 3){
			ret = moveLiftUpAndHold(target, FULLPOWER);
			if (!ret){
				return;
			}
		}
	}
	else {

		// ret = moveLiftUpAndHold(target, FULLPOWER);
		writeDebugStreamLine("100");
		ret = moveLiftUpAndHold(target, FULLPOWER);
		if (!ret){
			return;
		}
	}

	writeDebugStreamLine("103");

	// CLAW LIFT UP
	moveClawLiftUpAndStop(MAX_CLAWLIFT, FULLPOWER);
	writeDebugStreamLine("104");
	waitUntil(getClawLiftPosition() >= MAX_CLAWLIFT);
	writeDebugStreamLine("105");


	// int tcur = getLiftPosition();
	// writeDebugStreamLine("LiftDownPickUpClawLiftUpLiftDown) CONE_COUNT: %d, target: %d, lift: %d", CONE_COUNT, target,  tcur);




	// PUSH CONE DOWN
	if (CONE_COUNT < 2){
		moveLiftDownAndStop_Touch(powerliftDown);
		// moveLiftDownAndStop(MIN_LIFT, powerliftDown);
		openClaw(FULLPOWER);
		wait1Msec(100);
		stopClaw();
	}
	else
	{
		// wait1Msec(100);

		int tPower = FULLPOWER;
		int toff = 65;

		if (manualFeed){
			toff = 100;
			tPower = FULLPOWER;
		}

		// writeDebugStreamLine("toff: %d, tPower: %d", toff, tPower);
		int tcur = getLiftPosition();
		ret = moveLiftDownAndStop(tcur - toff, tPower);

		if (!ret){
			return;
		}

		stopClaw();
	}


	LIFT_STATUS = "";
}


bool scoreStationaryGoal_Part1()
{
	writeDebugStreamLine("part1");

	if (CONE_COUNT < 2){
		return false;
	}

	writeDebugStreamLine("MG: %d, SG: %d", CONE_COUNT,  CONE_COUNT_STATIONARY);

	int offset = getLiftTargetPosition(false);
	int targetMG = MIN_LIFT + offset;

	offset = getLiftTargetPositionForStationaryGoal();
	int targetSG = STATIONARY_LIFT + offset + 300;

	bool ret = moveLiftDownAndStop(targetMG - 50, powerliftDown);
	if (!ret){
		return false;
	}

	closeClaw(FULLPOWER / 2);
	wait1Msec(300);
	closeClaw(5);

	int tcur = getLiftPosition();

	if ((tcur + 100) < targetSG){
		ret = moveLiftUpAndHold(targetSG, FULLPOWER);
	}
	else
	{
		ret = moveLiftUpAndHold(tcur + 300, FULLPOWER);
	}

	if (!ret){
		return false;
	}

	moveLift(20);
	moveClawLiftDown(powerClawLiftDown);
	wait1Msec(500);
	ret = moveClawLiftDownAndStop(MIN_CLAWLIFT, powerClawLiftDown / 2);
	if (!ret){
		return false;
	}

	return true;
}
bool scoreStationaryGoal_Part2()
{
	writeDebugStreamLine("part2");

	if (CONE_COUNT < 2){
		return false;
	}

	int offset = getLiftTargetPosition(false);
	//int targetMG = MIN_LIFT + offset;

	offset = getLiftTargetPositionForStationaryGoal();
	int targetSG = STATIONARY_LIFT + offset;

	bool ret = moveLiftDownAndStop(targetSG - 0, powerliftDown);

	if (!ret){
		return false;
	}

	wait1Msec(100);

	openClaw(FULLPOWER);

	wait1Msec(300);

	CONE_COUNT_STATIONARY++;
	CONE_COUNT--;

	int tcur = getLiftPosition();

	ret = moveLiftUpAndStop(tcur + 100, FULLPOWER);
	if (!ret){
		return false;
	}

	stopClaw();

	wait1Msec(300);

	ret = moveClawLiftUpAndStop(MAX_CLAWLIFT, FULLPOWER);

	if (!ret){
		return false;
	}

	return true;
}

void moveLiftUpForStationaryGoal(int liftValue)
{

	bool ret = moveClawLiftDownAndStop(MIN_CLAWLIFT, powerClawLiftDown);
	if (!ret){
		return;
	}

	ret = moveLiftDownAndHold_Touch(FULLPOWER);
	if (!ret){
		return;
	}

	closeClaw(FULLPOWER);

	// PICK UP CONE AND HOLD
	wait1Msec(350);

	// CONE HOLD
	closeClaw(10);
	// CLAW LIFT UP
	moveClawLiftUpAndStop(MAX_CLAWLIFT, FULLPOWER);
	wait1Msec(100);

	while(true)
	{
		int offset = getLiftTargetPositionForStationaryGoal();
		int target = liftValue + offset;
		bool ret = true;
		bool adjustHeight = false;

		// LIFT UP
		// ret = moveLiftUpAndHold_Smart(target, FULLPOWER);
		ret = moveLiftUpAndHold(target, FULLPOWER);
		if (!ret){
			return;
		}

		int tcur = getLiftPosition();
		writeDebugStreamLine("MoveLiftUpForStationaryGoal) CONE_COUNT: %d, target: %d, lift: %d", CONE_COUNT, target,  tcur);


		while (getStaionaryGoal() == 0){

			if (vexRT[Btn8L] == 1)
			{
				return;
			}

			if (vexRT[Btn7U] == 1)
			{
				CONE_COUNT_STATIONARY++;
				adjustHeight = true;
				break;
			}
			else if (vexRT[Btn7D] == 1)
			{
				CONE_COUNT_STATIONARY--;
				adjustHeight = true;
				break;
			}
		}

		if (!adjustHeight){
			tcur = getLiftPosition();
			ret = moveLiftDownAndStop(tcur - 50, powerliftDown);

			if (!ret){
				return;
			}

			wait1Msec(100);

			openClaw(FULLPOWER);
			wait1Msec(300);

			stopClaw();

			ret = moveLiftUpAndHold(target, FULLPOWER);
			if (!ret){
				return;
			}

			CONE_COUNT_STATIONARY++;
			break;
		}
	}

	// MoveIEM()
}

void moveLiftUpForStationaryGoal1(int liftValue)
{
	int offset = getLiftTargetPositionForStationaryGoal();
	int target = liftValue + offset;
	bool ret = true;

	// PICKUP
	closeClaw(FULLPOWER);

	ret = moveLiftDownAndHold_Touch(powerliftDown);
	if (!ret){
		return;
	}


	// PICK UP CONE AND HOLD
	wait1Msec(300);

	// CONE HOLD
	closeClaw(10);

	moveClawLiftUp(FULLPOWER);

	wait1Msec(100);

	// LIFT UP
	// ret = moveLiftUpAndHold_Smart(target, FULLPOWER);
	ret = moveLiftUpAndHold(target, FULLPOWER);
	if (!ret){
		return;
	}

	int tcur = getLiftPosition();
	writeDebugStreamLine("MoveLiftUpForStationaryGoal) CONE_COUNT: %d, target: %d, lift: %d", CONE_COUNT, target,  tcur);


	// CLAW LIFT UP
	moveClawLiftUpAndStop(MAX_CLAWLIFT, FULLPOWER);

	while (getStaionaryGoal() == 0){

		if (vexRT[Btn8L] == 1)
		{
			return;
		}
	}

	tcur = getLiftPosition();
	ret = moveLiftDownAndStop(tcur - 50, powerliftDown);

	if (!ret){
		return;
	}

	wait1Msec(100);

	openClaw(FULLPOWER);
	wait1Msec(300);

	stopClaw();

	ret = moveLiftUpAndStop(target, FULLPOWER);
	if (!ret){
		return;
	}

	CONE_COUNT_STATIONARY++;
}

void moveLiftUpForStationaryGoalAuton(int liftValue)
{
	int offset = getLiftTargetPositionForStationaryGoal();
	int target = liftValue + offset;
	bool ret = true;

	// PICKUP
	//closeClaw(FULLPOWER);

	/*ret = moveLiftDownAndHold_Touch(powerliftDown);
	if (!ret){
	return;
	}


	// PICK UP CONE AND HOLD
	wait1Msec(300);*/

	// CONE HOLD
	closeClaw(10);

	moveClawLiftUp(FULLPOWER);

	wait1Msec(100);

	// LIFT UP
	// ret = moveLiftUpAndHold_Smart(target, FULLPOWER);
	ret = moveLiftUpAndHold(target, FULLPOWER);
	if (!ret){
		return;
	}

	int tcur = getLiftPosition();
	writeDebugStreamLine("MoveLiftUpForStationaryGoal) CONE_COUNT: %d, target: %d, lift: %d", CONE_COUNT, target,  tcur);


	// CLAW LIFT UP
	moveClawLiftUpAndStop(MAX_CLAWLIFT, FULLPOWER);

	while (getStaionaryGoal() == 0){

		if (vexRT[Btn8L] == 1)
		{
			return;
		}
	}
	MoveHelper(0);
	wait1Msec(500);
	tcur = getLiftPosition();
	ret = moveLiftDownAndStop(tcur - 50, powerliftDown);

	if (!ret){
		return;
	}

	wait1Msec(100);

	openClaw(FULLPOWER);
	wait1Msec(300);

	stopClaw();

	ret = moveLiftUpAndStop(target, FULLPOWER);
	if (!ret){
		return;
	}

	CONE_COUNT_STATIONARY++;
}

task clawLift()
{
	while (true)
	{
		wait1Msec(50);


		if (vexRT[Btn8R] == 1) {
			if (vexRT[Btn5U] == 1){
				moveClawLiftUpAndStop(MAX_CLAWLIFT, FULLPOWER);
			}
			else if (vexRT[Btn5D] == 1){
				moveClawLiftDownAndStop(MIN_CLAWLIFT, powerClawLiftDown);
			}
		}
		else if (vexRT[Btn8R] == 0) {

			if (vexRT[Btn5U] == 1){
				LIFT_STATUS = "manual";
				moveClawLiftUp(powerClawLiftUp);
			}
			else if (vexRT[Btn5D] == 1){
				LIFT_STATUS = "manual";
				moveClawLiftDown(powerClawLiftDown);
			}
		}

		if (LIFT_STATUS == "manual" && vexRT[Btn5U] == 0 && vexRT[Btn5D] == 0){
			stopClawLift();
		}
	}
}

task claw()
{
	while (true)
	{
		wait1Msec(50);

		if (vexRT[Btn8R] == 0) {
			if (vexRT[Btn6U] == 1){

				closeClaw(60);

				// int liftPos = getLiftPosition();
				// int clawLiftPos = getClawLiftPosition();

				// if (liftPos < MIN_LIFT && clawLiftPos < MIN_CLAWLIFT){
				// 	closeClaw(FULLPOWER);
				// 	bool ret = moveLiftDownAndHold_Touch(FULLPOWER);
				// 	if (!ret){
				// 		moveLiftUpAndStop(liftPos + 20, 30);
				// 		// return;
				// 	}
				// }
			}
			else if (vexRT[Btn6D] == 1){
				openClaw(POWER_CLAW_OPEN);
				wait1Msec(250);
				stopClaw();
			}
		}
	}
}


task usercontrol()
{
	startTask(move);
	startTask(lift);
	startTask(claw);
	startTask(clawLift);
	// User control code here, inside the loop
	//int on = 0;
	while (true)
	{
		wait1Msec(50);


		int btnLCD = nLCDButtons;
		if (btnLCD > 0)
		{
			AutonomousSelector(btnLCD);
			// startTask(displayBatteryLevelOnLCD);
		}

		while(vexRT[Btn8L] == 1){
			stopAll();
		}
		while (vexRT[Btn8R] == 1) {
			dropCone();
		}


	}
}

