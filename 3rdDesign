/*---------------------------------------------------------------------------*/
/*                                                                           */
/*                              User Control Task                            */
/*                                                                           */
/*  This task is used to control your robot during the user control phase of */
/*  a VEX Competition.                                                       */
/*                                                                           */
/*  You must modify the code to add your own robot specific commands here.   */
/*---------------------------------------------------------------------------*/

task move()
{
	while (true)
	{
		wait1Msec(50);

		int ch1 = vexRT[Ch1];
		int ch3 = vexRT[Ch3];

		if (autonomousMode == drivingSkillMode)
		{
			ch1 = ch1;
			//ch3 = ch3 * 2 / 3; //fastest pair was 3/5 for turning and 4/5 for going back and forth
		}

		motor[wheelLeft] = -ch3 - ch1;
		motor[wheelRight] = ch3 - ch1;


		if (abs(motor[wheelRight]) < 17 || abs(motor[wheelLeft]) < 17)
		{
			motor[wheelLeft] = 0;
			motor[wheelRight] = 0;
		}
	}
}


task lift()
{
	while (true)
	{
		wait1Msec(50);

		if (vexRT[Btn8R] == 1) {
			if (vexRT[Btn7U] == 1) { //Move Lift up with btn 7U
				LIFT_STATUS = "manual";
				moveLiftUp(FULLPOWER); //80
			}

			else if (vexRT[Btn7D] == 1){ //Move Lift down with btn 7D
				LIFT_STATUS = "manual";
				// moveLiftDownAndHold_Touch(powerliftDown); //-40
				moveLiftDown(FULLPOWER); //-40

			}
			else if (vexRT[Btn7R] == 1){
				moveMobileLiftDownAndStop();
				// moveMobileLiftDownToPickUp();
			}
			else if (vexRT[Btn7L] == 1){
				moveMobileLiftUpAndStop();
			}
			else if (vexRT[Btn6U] == 1){
				if (CONE_COUNT < 1){
					moveLiftUpForStationaryGoal(STATIONARY_LIFT);
				}
				else
				{
					int clawLiftPosition = getClawLiftPosition();
					int liftPosition = getLiftPosition();

					writeDebugStreamLine("6U> liftPosition: %d", liftPosition);

					int targetSG = getLiftTargetPositionForStationaryGoal_Reverse();
					if (clawLiftPosition >= MID_CLAWLIFT){
						if (STATIONARY_GOAL_TARGET == 0){
							// writeDebugStreamLine("");
							scoreStationaryGoal_Prepare();
						}
						else
						{
							scoreStationaryGoal_Part1(targetSG + 100);
						}
					}
					else if (clawLiftPosition < MID_CLAWLIFT)
					{
						bool ret = scoreStationaryGoal_Part2(liftPosition + 100);
						if (ret){
							getLiftTargetPositionForStationaryGoal_Reverse_Increase();
						}
					}

				}
			}
			else if (vexRT[Btn6D] == 1){
				if (CONE_COUNT < 1){
					moveLiftUpForStationaryGoal(STATIONARY_LIFT);
				}
				else
				{
					//bool ret = true;

					while(CONE_COUNT > 1) {
						bool cont = false;
						long startTime = nPgmTime;

						int liftPosition = getLiftPosition();

						writeDebugStreamLine("6D> liftPosition: %d", liftPosition);


						int targetSG = getLiftTargetPositionForStationaryGoal_Reverse();

						bool ret = scoreStationaryGoal_Part1(targetSG + 150);
						if (ret){
							ret = scoreStationaryGoal_Part2(liftPosition + 100);
							if (ret){
								cont = true;

								getLiftTargetPositionForStationaryGoal_Reverse_Increase();

								clearTimer(T1);

								while(time1[T1] < 300){
									// writeDebugStreamLine("CONE_COUNT: %d", CONE_COUNT);

									if (vexRT[Btn8L] == 1){
										writeDebugStreamLine("8L");
										cont = false;
										break;
									}
								}
							}
						}

						if (!cont){
							writeDebugStreamLine("break");
							break;
						}

						writeDebugStreamLine("continue) CONE_COUNT: %d, Time: %d", CONE_COUNT, nPgmTime - startTime);


						// ret = scoreStationaryGoal_Part1();
						// if (!ret){
						// 	break;
						// }
						// ret = scoreStationaryGoal_Part2();
						// if (!ret){
						// 	break;
						// }
					}

					stopAll();
				}
			}


			// if (vexRT[Btn5U] == 0 && vexRT[Btn5D] == 0){
			// 	stopClawLift();
			// }

			if (LIFT_STATUS == "manual" && vexRT[Btn7U] == 0 && vexRT[Btn7D] == 0){
				stopLift();
			}

			if (vexRT[Btn7R] == 0 && vexRT[Btn7L] == 0){
				stopMobileLift();
			}
		}
		else if (vexRT[Btn8R] == 0) {
			if (vexRT[Btn7U] == 1) {


				CONE_COUNT++;
				int liftTarget = getLiftTargetPosition(false);
				liftTarget = liftTarget + MIN_LIFT;
				int clawLiftPosition = getClawLiftPosition();
				// int liftTarget = getNewLiftTargetPosition(false, true);

				// int liftPosition = getLiftPosition();

				// if (liftPosition < MIN_LIFT && clawLiftPosition < MIN_CLAWLIFT){
				// 	LiftDownPickUpClawLiftUpLiftDown_PART1_2();
				// }
				// else
				if (clawLiftPosition >= MID_CLAWLIFT){
					LiftDownPickUpClawLiftUpLiftDown_PART1_1(liftTarget, false);
					LiftDownPickUpClawLiftUpLiftDown_PART2_1(liftTarget, false);
				}
				else if (clawLiftPosition < MID_CLAWLIFT)
				{
					LiftDownPickUpClawLiftUpLiftDown_PART2_1(liftTarget, false);
				}
			}
			else if (vexRT[Btn7D] == 1) {


				int liftTarget = getLiftTargetPosition(false);
				liftTarget = liftTarget + MIN_LIFT;

				int clawLiftPosition = getClawLiftPosition();
				// int liftTarget = getCurrentLiftTargetPosition(false);

				// int liftPosition = getLiftPosition();

				// if (liftPosition < MIN_LIFT && clawLiftPosition < MIN_CLAWLIFT){
				// 	LiftDownPickUpClawLiftUpLiftDown_PART1_2();
				// }
				// else
				if (clawLiftPosition >= MID_CLAWLIFT){
					LiftDownPickUpClawLiftUpLiftDown_PART1_1(liftTarget, false);
					LiftDownPickUpClawLiftUpLiftDown_PART2_1(liftTarget, false);
				}
				else if (clawLiftPosition < MID_CLAWLIFT)
				{
					LiftDownPickUpClawLiftUpLiftDown_PART2_1(liftTarget, false);
				}
			}
			else if (vexRT[Btn7L] == 1){
				if (autonomousMode == drivingSkillMode){
					moveMobileLiftUpAndStopDrivingSkill();
				}
				else{
					moveMobileLiftUpAndStop();
				}

			}
			else if (vexRT[Btn7R] == 1){
				writeDebugStreamLine("7R");

				moveMobileLiftDownToPickUp();
			}
			else if (vexRT[Btn8U] == 1){
				long startTime = nPgmTime;

				bool cont = true;

				writeDebugStreamLine("IN ? ");

				while(cont && CONE_COUNT < 12){
					writeDebugStreamLine("IN");
					cont = false;
					CONE_COUNT++;
					int liftTarget = getLiftTargetPosition(true);
					// liftTarget = liftTarget + FEED_LIFT;
					liftTarget = liftTarget + MIN_LIFT;

					bool ret = LiftDownPickUpClawLiftUpLiftDown_PART1_1(liftTarget, true);
					if (ret){
						ret = LiftDownPickUpClawLiftUpLiftDown_PART2_1(liftTarget, true);
						if (ret){
							clearTimer(T1);
							while(time1[T1] < 300){
								cont = true;
								// writeDebugStreamLine("CONE_COUNT: %d", CONE_COUNT);

								if (vexRT[Btn8L] == 1){
									writeDebugStreamLine("8L");
									cont = false;
									break;
								}
								else if (vexRT[Btn7U] == 1){
									writeDebugStreamLine("7U");
									CONE_COUNT++;
									break;
								}
								else if (vexRT[Btn7D] == 1){
									writeDebugStreamLine("7D");
									CONE_COUNT--;
									break;
								}
							}
						}
					}


					writeDebugStreamLine("ManualFeed) CONE_COUNT: %d, Time: %d", CONE_COUNT, nPgmTime - startTime);

				}

				writeDebugStreamLine("OUT");


			}
			else if (vexRT[Btn8D] == 1){
				// autonRed();
				CONE_COUNT++;
				int liftTarget = getLiftTargetPosition(true);
				// liftTarget = liftTarget + FEED_LIFT;
				liftTarget = liftTarget + MIN_LIFT;



				// writeDebugStreamLine("1");
				bool ret = LiftDownPickUpClawLiftUpLiftDown_PART1_1(liftTarget, true);
				if (ret){
					LiftDownPickUpClawLiftUpLiftDown_PART2_1(liftTarget, true);
				}
			}

			if (vexRT[Btn7R] == 0 && vexRT[Btn7L] == 0){
				stopMobileLift();
			}
		}
	}
}

bool LiftDownPickUpClawLiftUpLiftDown_PART1_1(int target, bool manualFeed)
{
	writeDebugStreamLine("PART1> Target: %d, Cone: %d", target, CONE_COUNT);

	LIFT_STATUS = "LiftDownPickUpClawLiftUpLiftDown_PART1_1";
	// READY PICKUP
	openClaw(FULLPOWER);
	wait1Msec(300);

	// int target = getNewLiftTargetPosition(manualFeed);

	int clawLiftPosition = getClawLiftPosition();
	// writeDebugStreamLine("clawLiftDown: %d", clawLiftDown);

	bool ret = true;

	// writeDebugStreamLine("200");
	if (clawLiftPosition >= MID_CLAWLIFT && CONE_COUNT > 0)
	{
		// writeDebugStreamLine("200-1");

		// moveLiftUpAndHold(target, FULLPOWER);
		ret = PushORUpCone(false);

		if (!ret){
			return false;
		}

	}

	// writeDebugStreamLine("201");

	stopClaw();


	// startTask(moveClawLiftDownAndStopTask);

	// waitUntil(getClawLiftPosition() >= MAX_CLAWLIFT);
	// writeDebugStreamLine("203");


	ret = moveClawLiftDownAndStop(MIN_CLAWLIFT, HALFFULLPOWER /*powerClawLiftDown*/);
	if (!ret){
		return false;
	}

	// ret = moveClawLiftDownAndContinue(MID_CLAWLIFT, powerClawLiftDown);

	// if (!ret){
	// 	return false;
	// }

	// startTask(moveClawLiftDownAndStopTask);

	// if (!manualFeed)
	// {
	// 	// writeDebugStreamLine("203");

	// 	bool ret = moveLiftDownAndStop(MIN_LIFT, powerliftDown + 30);
	// 	// bool ret = moveLiftDownAndStop_Touch(powerliftDown + 30);
	// 	if (ret) {

	// 		closeClaw(60);
	// 		int tcur = getLiftPosition();
	// 		moveLiftUpAndStop(tcur + 20, 30);
	// 	}
	// }

	LIFT_STATUS = "";

	return true;
}

